{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "/home/edmondium/root/bin/root.exe",
      "args": [
        "-l",
        "-q",
        "macro8.c+g",
        "hstack.c+g",
        "macro7.c+g",
        "macro6.c+g",
        "macro5.c+g",
        "multigraph.c+g",
        "macro4.c+g",
        "macro3.c+g",
        "macro2.c+g",
        "macro1.C+g",
        "slits.C+g",
        "hsimple.C+g"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
